-- FreeVersionallowedGamesByCreatorId
-- FreeVersionallowedGamesByPlaceId
-- allowedGamesforPremiumByCreatorId
-- AllowGameforPremiumByPlaceId

local placeId = tostring(game.PlaceId)
local creatorId = tostring(game.CreatorId)
local gameData = 
    AllowGameforPremiumByPlaceId[placeId] or
    allowedGamesforPremiumByCreatorId[tonumber(creatorId)] or
    FreeVersionallowedGamesByPlaceId[placeId] or
    FreeVersionallowedGamesByCreatorId[tonumber(creatorId)]
if not gameData then
    notify("‚ùå This script is not supported in this game!")
    wait(4)
    player:Kick("‚ö†Ô∏è Script not supported here.\nüìä Please run the script in supported games.\nüîó Join our (dsc.gg/dyhub)")
    return
end
local function loadScript()
    if gameData.url then
        local success, err = pcall(function()
            loadstring(game:HttpGet(gameData.url))()
        end)
        if success then
            notify("üéÆ Game: " .. gameData.name .. " | Script loaded.")
        else
            notify("‚ùå Failed to load script: " .. tostring(err))
        end
    else
        notify("‚ÄºÔ∏è No script available for this game!")
    end
end
loadScript()
