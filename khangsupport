
-- ================== QUEUE TELEPORT ==================
local Players = game:GetService("Players")
local TeleportCheck = false
local KeepInfYield = true -- ← Gán biến này là true để bật tính năng
local queueteleport = queue_on_teleport or queue_on_teleport or syn and syn.queue_on_teleport -- Phổ biến nhất

Players.LocalPlayer.OnTeleport:Connect(function(State)
if KeepInfYield and not TeleportCheck and queueteleport then

queueteleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/idtkby/NowGeta/main/khangsupport'))()")
end
end)


-- SERVICES
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local MarketplaceService = game:GetService("MarketplaceService")

-- NOTIFY
local function notify(text)
    pcall(function()
        StarterGui:SetCore("SendNotification", {
            Title = "Waiting...",
            Text = text,
            Duration = 5
        })
    end)
end

-- LOADER
local function load(url)
    task.wait(0.3)
    loadstring(game:HttpGet(url))()
end

local pid = game.PlaceId

-- GAME CHECK
if pid == 70876832253163 then
    notify("Loading Dead Rails")
    load("https://raw.githubusercontent.com/idtkby/Xd/refs/heads/main/Dead%20rails%20hub")

elseif pid == 115345163333015 or pid == 1985320156 then
    notify("Loading Eyes")
    load("https://raw.githubusercontent.com/idtkby/Xd/refs/heads/main/Eyes%20hub")

elseif pid == 121308443347459 or pid == 17474746614 then
    notify("Loading 7 Days")
    load("https://raw.githubusercontent.com/idtkby/Xd/refs/heads/main/7DaysForest")

elseif pid == 116387287343653 then
    notify("Loading PETA School")
    load("https://raw.githubusercontent.com/idtkby/Xd/refs/heads/main/PETAPETA%3A%20School%20of%20Nightmares%20UI")

elseif pid == 14476003462 then
    notify("Loading PETA Lv6")
    load("https://raw.githubusercontent.com/idtkby/Xd/refs/heads/main/PETAPETA%7C%20Level%206%20UI")

elseif pid == 104592421039274 then
    notify("Loading AOONI")
    load("https://raw.githubusercontent.com/idtkby/Xd/refs/heads/main/AOONI%20Nightmare%20UI")

elseif pid == 18687417158 then
    notify("Loading Forsaken")
    load("https://raw.githubusercontent.com/idtkby/Xd/main/forsaken.lua")

elseif pid == 5247067896 or pid == 4623386862 or pid == 5661005779 then
    notify("Loading Piggy")
    load("https://raw.githubusercontent.com/idtkby/Xd/main/piggy%20gui")

elseif pid == 99078474560152 then
    notify("Loading M.E.G")
    load("https://raw.githubusercontent.com/idtkby/Xd/main/M.e.g")

elseif pid == 138103330716004 then
    notify("Loading After 3 AM")
    load("https://raw.githubusercontent.com/idtkby/Xd/refs/heads/main/After3am.lua")

else
    -- CHECK TÊN GAME: SPECTER
    local name = ""
    pcall(function()
        name = MarketplaceService:GetProductInfo(pid).Name
    end)

    if string.find(string.upper(name), "SPECTER") then
        notify("Loading SPECTER")
        load("https://raw.githubusercontent.com/idtkby/Xd/main/specter2")
    else
        -- HUB CHUNG
        load("https://raw.githubusercontent.com/idtkby/Xd/refs/heads/main/KhangHub")
    end
end
